<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.recruit.mapper.AdminMapper">

    <resultMap id="BaseResultMap" type="com.example.recruit.domain.Admin">
            <id property="adminId" column="admin_id" jdbcType="INTEGER"/>
            <result property="adminName" column="admin_name" jdbcType="VARCHAR"/>
            <result property="adminPwd" column="admin_pwd" jdbcType="CHAR"/>
            <result property="unitId" column="unit_id" jdbcType="INTEGER"/>
            <result property="adminState" column="admin_state" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        admin_id,admin_name,admin_pwd,
        unit_id,admin_state
    </sql>
</mapper>
