<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.recruit.mapper.UnitMapper">

    <resultMap id="BaseResultMap" type="com.example.recruit.domain.Unit">
        <id property="unitId" column="unit_id" jdbcType="INTEGER"/>
        <result property="unitName" column="unit_name" jdbcType="VARCHAR"/>
        <result property="regionId" column="region_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        unit_id
        ,unit_name,region_id
    </sql>


</mapper>
